   "source": [
    "# Sample size\n",
    "n = 1000; \n",
    "# Sample from a normal distribution\n",
    "u= sc.randn(n);\n",
    "nBars=10; uMin=-5; uMax=5; uStep=0.5\n",
    "uBins= sc.arange(uMin,uMax, uStep)\n",
    "#print(x)\n",
    "fig=gr.figure(figsize=(13,7))\n",
    "rows=1; cols=3\n",
    "axSample = fig.add_subplot(rows,cols,1)\n",
    "axHist1 = fig.add_subplot(rows,cols,2)\n",
    "axHist2 = fig.add_subplot(rows,cols,3)\n",
    "axSample.plot(u, '.')\n",
    "# Histogram with a predefine number of bars\n",
    "axHist1.hist(u,nBars, orientation=\"horizontal\")\n",
    "axHist2.set_ylim(uMin,uMax)\n",
    "# Histogram with bars located at a previously defined set of points\n",
    "axHist2.hist(u,uBins, orientation=\"horizontal\")"
   ]
